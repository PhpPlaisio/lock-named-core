{
  "name": "plaisio/lock-named-core",
  "description": "PhpPlaisio: Core Named Lock",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Named Lock"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "plaisio/kernel": "^3.2.1",
    "plaisio/lock-named": "^1.3.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phing/phing": "^3.0.0",
    "phpunit/phpunit": "^9.6.3",
    "plaisio/company-resolver-uni": "^2.1.1",
    "plaisio/console-kernel": "^1.0.1",
    "setbased/php-stratum": "^6.3.1",
    "setbased/php-stratum-mysql": "^6.6.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Lock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Lock\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
