{
  "name": "plaisio/lock-named-core",
  "description": "PhpPlaisio: Core Named Lock",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Named Lock"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.3",
    "plaisio/kernel": "^1.1",
    "plaisio/lock-named": "^1.1"
  },
  "require-dev": {
    "phing/phing": "^2.0",
    "phpunit/phpunit": "^9.0",
    "plaisio/company-resolver-uni": "^1.1",
    "setbased/php-stratum": "^5.4",
    "setbased/php-stratum-mysql": "^4.4"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Lock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Lock\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true
  }
}
