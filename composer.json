{
  "name": "plaisio/lock-named-core",
  "description": "PhpPlaisio: Core Named Lock",
  "keywords": [
    "PhpPlaisio",
    "Core",
    "Named Lock"
  ],
  "license": "MIT",
  "require": {
    "php": ">=7.1.0",
    "plaisio/kernel": "^1.0.0",
    "plaisio/lock-named": "^1.0.0"
  },
  "require-dev": {
    "phing/phing": "^2.0.0",
    "phpunit/phpunit": "^7.0.0 || ^8.0.0",
    "plaisio/company-resolver-uni": "^1.0.0",
    "setbased/php-stratum": "^5.0.0",
    "setbased/php-stratum-mysql": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "Plaisio\\Lock\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Plaisio\\Lock\\Test\\": "test/"
    },
    "classmap": [
      "test/C.php"
    ]
  },
  "config": {
    "bin-dir": "bin/"
  }
}
